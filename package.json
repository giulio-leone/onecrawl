{
  "name": "onecrawl",
  "version": "1.0.0",
  "description": "Native TypeScript web crawler and scraper. Zero Python dependencies.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "onecrawl": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./adapters": {
      "import": "./dist/adapters/index.js",
      "types": "./dist/adapters/index.d.ts"
    },
    "./ports": {
      "import": "./dist/ports/index.js",
      "types": "./dist/ports/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist",
    "cli": "node dist/cli.js"
  },
  "dependencies": {
    "playwright": "^1.50.0",
    "undici": "^7.19.1",
    "zod": "^3.24.1"
  },
  "optionalDependencies": {
    "turndown": "^7.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/turndown": "^5.0.5",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "crawler",
    "scraper",
    "web",
    "playwright",
    "typescript",
    "search",
    "browser",
    "cli"
  ],
  "author": "OneGenUI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/g97iulio1609/onecrawl.git"
  }
}
